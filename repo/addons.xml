<?xml version="1.0" encoding="UTF-8"?>
<addons>
  <addon id="plugin.program.lockdown" name="Lockdown Wizard" version="1.0" provider-name="lockdown">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
        <import addon="script.module.requests" />
        <import addon="script.module.six" />
    </requires>
    
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
    </extension>
    
    <extension point="xbmc.service" library="startup.py" start="startup" />
    
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Lockdown Wizard</summary>
        <description lang="en">Lockdown Wizard</description>
        <platform>all</platform>
        <reuselanguageinvoker>false</reuselanguageinvoker>
        <source>https://lockdown/</source>
        <news>
        </news>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.png</fanart>
        </assets>
    </extension>
</addon>

<addon id="repository.lockdown"
       name="lockdown"
       version="1.1.2"
       provider-name="Lockdown_Machine">
    <extension point="xbmc.addon.repository" name="Lockdown Repository">
    <dir>
        <info compressed="false">https://raw.githubusercontent.com/SorryNotSorryNot/lockdown/repo/master/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/SorryNotSorryNot/lockdown/repo/master/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/SorryNotSorryNot/lockdown/master/repo/</datadir>
    </dir>
    <dir>
        <info compressed="false">https://raw.githubusercontent.com/hydem98/addons/master/zips/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/hydem98/addons/master/zips/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/hydem98/addons/master/zips/</datadir>
    </dir>
    <dir>
        <info compressed="false">https://raw.githubusercontent.com/jsergio123/zips/master/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/jsergio123/zips/master/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/jsergio123/zips/master/</datadir>
    </dir>
